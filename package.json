{
  "name": "vue-system-demo",
  "version": "1.0.0",
  "description": "准备用该系统尝试制作基于vue的前后端管理平台,该项目目前仅用于学习和测试",
  "author": "viccowang <viccowang@gmail.com>",
  "private": true,
  "scripts": {
    "dev:admin": "cross-env NODE_ENV_TYPE=admin webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:client": "set NODE_ENV_TYPE=client && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build:admin": "cross-env NODE_ENV_TYPE=admin node build/build.js",
    "build:client": "cross-env NODE_ENV_TYPE=client node build/build.js",
    "analyz:admin": "cross-env npm_config_report=true npm run build:admin",
    "analyz:client": "cross-env npm_config_report=true npm run build:client",
    "start": "npm run dev:admin",
    "lint": "eslint --ext .js,.vue src"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "element-ui": "^2.3.8",
    "moment": "^2.20.1",
